    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\src\\app.js",
            "restart": true,
            "runtimeExecutable": "nodemon",
            "env":{
                "MONGO_URL":"BASE",
                "GOOGLE_CLIENT":"GOOGLE_CLIENT",
                "GOOGLE_SECRET":"GOOGLE_SECRET",
                "GOOGLE_KEY_FILE":"FILE KEY",
                "GOOGLE_BUCKET_NAME":"BUCKET",
                "JWT_COOKIE":"authCookie",
                "JWT_SECRET":"jwtSecret",
                "ADMIN_EMAIL":"correoadmin@correo.com",
                "ADMIN_PASSWORD":"123",
                "NODE_MAILER_USER":"MAILER CLIENT",
                "NODE_MAILER_PASSWORD":"MAILER PASSWORD"
            }
        }
    ]
}